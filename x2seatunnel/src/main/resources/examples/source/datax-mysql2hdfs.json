{
  "job": {
    "setting": {
      "speed": {
        "channel": 3
      }
    },
    "content": [
      {
        "reader": {
          "name": "mysqlreader",
          "parameter": {
            "username": "root",
            "password": "1234567",
            "connection": [
              {
                "jdbcUrl": ["jdbc:mysql://localhost:3306/testdb"],
                "table": ["users"]
              }
            ],
            "column": ["id", "name", "age", "email", "create_time"],
            "splitPk": "id"
          }
        },
        "writer": {
          "name": "hdfswriter",
          "parameter": {
            "defaultFS": "hdfs://localhost:9000",
            "path": "/data/users",
            "fileName": "users_export_${now}",
            "fileType": "text",
            "fieldDelimiter": "\t",
            "rowDelimiter": "\n",
            "writeMode": "append",
            "compress": "gzip",
            "encoding": "UTF-8",
            "batchSize": 50000
          }
        }
      }
    ]
  }
}
